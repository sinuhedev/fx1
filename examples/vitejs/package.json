		"test:coverage": "vitest --silent --coverage"
	},
	"devDependencies": {
		"@vitejs/plugin-react": "^4.4.1",
		"@vitest/coverage-v8": "^3.1.3",
		"autoprefixer": "^10.4.21",
		"jsdom": "^26.1.0",
		"vite": "^6.3.5",
		"vitest": "^3.0.4"
	},
	"dependencies": {
		"nextia": "file:../..",
		"react": "^19.1.0",
		"react-dom": "^19.1.0"
